  "porosity_effects": {
    "impact_energy_absorption": {
      "description": "High porosity increases energy absorption, reducing damage efficiency",
      "formula": "efficiency = 1.0 - 0.6 * (porosity / 100)",
      "note": "Porous asteroids compress on impact, dissipating kinetic energy"
    },
    "atmospheric_fragmentation": {
      "description": "Porous bodies fragment at higher altitudes due to lower tensile strength",
      "formula": "breakup_altitude_km = base_altitude * (1 + porosity/100)",
      "note": "Explains Chelyabinsk airburst at ~30km altitude"
    },
    "shock_wave_propagation": {
      "description": "Porosity dampens shock wave intensity",
      "formula": "shock_attenuation = exp(-damping_factor * porosity/100)",
      "note": "Critical for seismic effects calculation"
    },
    "crater_scaling": {
      "description": "Bulk density affects crater size via momentum transfer",
      "formula": "crater_diameter âˆ (projectile_density/target_density)^(1/3)",
      "note": "Low density impactor = smaller crater for same size"
    }
  },
