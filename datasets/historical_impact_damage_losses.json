  "casualty_estimation_models": {
    "description": "Methods to estimate casualties from impact parameters",
    "immediate_casualties": {
      "fireball_thermal_burns": {
        "formula": "casualties = population_density * area_circle(thermal_radius_3rd_degree_burns)",
        "lethality": 0.5,
        "note": "People in direct line of sight to fireball"
      },
      "blast_overpressure": {
        "overpressure_50kpa": {"structural_collapse": "major", "lethality": 0.95},
        "overpressure_35kpa": {"structural_collapse": "severe", "lethality": 0.80},
        "overpressure_20kpa": {"structural_collapse": "moderate", "lethality": 0.50},
        "overpressure_10kpa": {"structural_collapse": "minor", "lethality": 0.15},
        "overpressure_3kpa": {"window_breakage": "widespread", "lethality": 0.05},
        "formula": "casualties = sum(population(r) * lethality(overpressure(r)))"
      },
      "crater_formation": {
        "formula": "casualties = population_inside_crater_radius * 1.0",
        "note": "100% fatality within crater diameter"
      },
      "ejecta_blanket": {
        "formula": "casualties = population(ejecta_radius) * 0.8",
        "ejecta_radius_formula": "R_ejecta = 3 * R_crater",
        "note": "High lethality from ballistic ejecta"
      }
    },
    "secondary_casualties": {
      "building_collapse": {
